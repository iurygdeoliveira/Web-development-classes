        "geocoder-php/chain-provider": "^4.4",
        "geocoder-php/bing-maps-provider": "^4.2",
        "react/react": "^1.2",
        "psr/http-factory": "^1.0.2",
        "php-curl-class/php-curl-class": "^9.6",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.17.0",
        "phpmetrics/phpmetrics": "^2.7",
        "icanhazstring/composer-unused": "^0.8.2",
        "filp/whoops": "^2.14"
