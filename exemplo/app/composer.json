{
    "name": "iuryprof/exemplo",
    "description": "Aplicacao de exemplo",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Iury Gomes de Oliveira",
            "email": "iury.oliveira@ifto.edu.br"
        }
